{
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "[%(asctime)s] [%(levelname)8s] [%(name)24s] [%(funcName)16s] - %(message)s"
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "formatter": "simple",
            "level": "INFO",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "asyncio": {
            "propagate": false
        },
        "websockets": {
            "propagate": false
        }
    },
    "root": {
        "handlers": [
            "stdout"
        ],
        "level": "DEBUG"
    },
    "version": 1
}